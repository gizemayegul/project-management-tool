{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite --port 5006 --host",
    "start:dev": "npm run dev",
    "start": "tsc && vite build --mode prod && vite preview",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:all": "jest --watch --verbose",
    "serve:prod": "tsc && vite build --mode prod && vite preview --host 0.0.0.0 --port 5007",
    "start:prod": "npm run serve:prod",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@chakra-ui/icons": "^2.1.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@heroicons/react": "^2.1.4",
    "@lottiefiles/dotlottie-react": "^0.8.6",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-query-devtools": "^5.60.5",
    "axios": "^1.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "vitest-canvas-mock": "^0.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.2",
    "daisyui": "^4.12.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.1.6",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "jsdom",
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ]
  }
}
